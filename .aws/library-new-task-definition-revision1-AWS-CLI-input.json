{
    "family": "library-new-task-definition",
    "containerDefinitions": [
        {
            "name": "library-new-container",
            "image": "414502573844.dkr.ecr.ap-southeast-1.amazonaws.com/library-new-repo:LATEST",
            "cpu": 512,
            "memory": 1024,
            "memoryReservation": 1024,
            "portMappings": [
                {
                    "name": "http-8080-port",
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/library-new-task-definition",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-southeast-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "exit 1"
                ],
                "interval": 5,
                "timeout": 2,
                "retries": 3,
                "startPeriod": 15
            },
            "systemControls": []
        }
    ],
    "executionRoleArn": "arn:aws:iam::414502573844:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}